{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "FullAccess",
      "Effect": "Allow",
	  "Action": [
		"iam:*",
		"s3:*",
		"apigateway:*",
		"lambda:*",
		"sns:*",
		"ses:*",
		"batch:*",
		"ecr:*",
		"ecs:*",
		"eks:*",
		"elasticache:*",
		"rds:*",
		"secretmanager:*",
		"kms:*",
		"acm:*",
		"ec2:*",
		"cloudformation:*",
		"cloudwatch:*",
		"cloudtrail:*",
		"guardduty:*",
		"appstream:*"
	  ],
      "Resource": "*"
    },
    {
      "Sid": "ReadOnly",
	  "Effect": "Allow",
      "Action": [
		"securityhub:Get*",
		"securityhub:List*",
		"securityhub:Describe*",
		"inspector:Get*",
		"inspector:List*",
		"inspector:Describe*"
      ],
      "Resource": "*"
    }
  ]
}