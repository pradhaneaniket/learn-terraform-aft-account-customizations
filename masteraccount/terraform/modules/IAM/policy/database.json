{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "FullAccess",
      "Effect": "Allow",
	  "Action": [
		"s3:*",
		"elasticache:*",
		"rds:*",
		"cloudwatch:*"
	  ],
      "Resource": "*"
    },
    {
      "Sid": "ReadOnly",
	  "Effect": "Allow",
      "Action": [
		"iam:Get*",
        "iam:List*",
		"sns:Get*",
        "sns:List*",
		"kms:Get*",
        "kms:List*",
		"ec2:Get*",
        "ec2:Describe*",
		"securityhub:Get*",
		"securityhub:List*",
		"securityhub:*",
		"inspector:Get*",
		"inspector:List*",
		"inspector:Describe*"
      ],
      "Resource": "*"
    }
  ]
}