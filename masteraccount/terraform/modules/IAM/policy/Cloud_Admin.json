            "Action": [
                "iam:*",
                "s3:*",
                "apigateway:*",
                "lambda:*",
                "sns:*",
                "ses:*",
                "batch:*",
                "ecr:*",
                "ecs:*",
                "eks:*",
                "elasticache:*",
                "rds:*",
                "secretmanager:*",
                "kms:*",
                "acm:*",
                "ec2:*",
                "cloudformation:*",
                "cloudwatch:*",
                "cloudtrail:*",
                "guardduty:*",
                "appstream:*",
                "inspector:*",
                "securityhub:*"
            ],
            "Resource": "*"
        }
    ]
}